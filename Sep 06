// 1 st Answer
package New;

public class IfPrint {
	static void number(int n) {
	if(n<=10) {
		System.out.print(n+" ");
		n++;
		number(n);
	}
	}
public static void main(String[] args) {
	int a=1;
	number(a);
}
}

// 2 nd Answer
package New;

public class WoForPrint {
public static void main(String[] args) {
	int i=1;
	while(i<=10) {
		System.out.print(i+" ");
		i++;
	}
	
}
}
