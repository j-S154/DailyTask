// 1 st Answer
let even=[];
let odd=[];
for (let i = 1; i <= 8; i++) {
    if(i%2==0){
    even.push(i);
    }
    else{
    odd.push(i);
    }
}
console.log("Even Numbers :")
even.forEach(number => {
    console.log(number);
});
console.log("Odd Numbers :")
odd.forEach(number => {
    console.log(number);
});

// 2 nd Answer
let num = [];

for (let i=1; i<=10;i++) {
    num.push(i);
}
num.forEach(number => {
    console.log(number);
});
